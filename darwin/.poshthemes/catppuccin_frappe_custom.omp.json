{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "closer": "p:os",
        "pink": "#F4B8E4",
        "lavender": "#BABBF1",
        "blue":  "#8CAAEE"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#C6D0F5",
          "background": "#626880",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "template": "B",
          "foreground": "#c6d0f5",
          "foreground_templates": [
            "{{ if eq (mod (now | unixEpoch) 6) 0 }}#FF5555{{ end }}",
            "{{ if eq (mod (now | unixEpoch) 6) 1 }}#FFB86C{{ end }}",
            "{{ if eq (mod (now | unixEpoch) 6) 2 }}#F1FA8C{{ end }}",
            "{{ if eq (mod (now | unixEpoch) 6) 3 }}#50FA7B{{ end }}",
            "{{ if eq (mod (now | unixEpoch) 6) 4 }}#8BE9FD{{ end }}",
            "{{ if eq (mod (now | unixEpoch) 6) 5 }}#BD93F9{{ end }}"
          ]
        },
        {
          "type": "text",
          "template": "i",
          "foreground": "#c6d0f5",
          "foreground_templates": [
            "{{ if eq (mod (add (now | unixEpoch) 1) 6) 0 }}#FF5555{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 1) 6) 1 }}#FFB86C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 1) 6) 2 }}#F1FA8C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 1) 6) 3 }}#50FA7B{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 1) 6) 4 }}#8BE9FD{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 1) 6) 5 }}#BD93F9{{ end }}"
          ]
        },
        {
          "type": "text",
          "template": "X",
          "foreground": "#c6d0f5",
          "foreground_templates": [
            "{{ if eq (mod (add (now | unixEpoch) 2) 6) 0 }}#FF5555{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 2) 6) 1 }}#FFB86C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 2) 6) 2 }}#F1FA8C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 2) 6) 3 }}#50FA7B{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 2) 6) 4 }}#8BE9FD{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 2) 6) 5 }}#BD93F9{{ end }}"
          ]
        },
        {
          "type": "text",
          "template": "K",
          "foreground": "#c6d0f5",
          "foreground_templates": [
            "{{ if eq (mod (add (now | unixEpoch) 3) 6) 0 }}#FF5555{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 3) 6) 1 }}#FFB86C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 3) 6) 2 }}#F1FA8C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 3) 6) 3 }}#50FA7B{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 3) 6) 4 }}#8BE9FD{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 3) 6) 5 }}#BD93F9{{ end }}"
          ]
        },
        {
          "type": "text",
          "template": "u",
          "foreground": "#c6d0f5",
          "foreground_templates": [
            "{{ if eq (mod (add (now | unixEpoch) 4) 6) 0 }}#FF5555{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 4) 6) 1 }}#FFB86C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 4) 6) 2 }}#F1FA8C{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 4) 6) 3 }}#50FA7B{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 4) 6) 4 }}#8BE9FD{{ end }}",
            "{{ if eq (mod (add (now | unixEpoch) 4) 6) 5 }}#BD93F9{{ end }}"
          ]
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": " \uf105 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}

